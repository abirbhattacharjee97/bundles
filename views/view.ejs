<!DOCTYPE html>
<html>
  <head>
    <title>My Table</title>
    <style>
      table {
        border-collapse: collapse;
        width: 100%;
        color: #333;
        font-family: sans-serif;
        font-size: 0.9em;
        text-align: left;
      }
      th, td {
        padding: 0.5em;
        border-bottom: 1px solid #ddd;
      }
      th {
        background-color: #eee;
      }
    </style>
  </head>

  <button onclick="closeWindow()">Close</button>

  <script>
    function closeWindow() {
      window.close();
    }
  </script>

  <body>
    <table>
      <thead>
        <tr>
          <th>bundle_id</th>
          <th>bundle</th>
          <th>request_id</th>
          <th>WEEKP</th>
          <th>CROP</th>
          <th>Value</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(function(row) { %>
        <tr>
          <td><%= row.bundle_id %></td>
          <td><%= row.bundle %></td>
          <td><%= row.request_id %></td>
          <td><%= row.WEEKP %></td>
          <td><%= row.CROP %></td>
          <td><%= row.Value %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

    <form action="/" method="POST">
		<label for="user_id">Enter user_id:</label>
		<input type="text" name="user_id" id="user_id">
  
		<label for="bundle">Enter bundle:</label>
		<input type="text" name="bundle" id="bundle">
  
		<label for="bid1">Enter bid for 1st crop:</label>
		<input type="text" name="bid1" id="bid1">

		<label for="bid2">Enter bid for 2nd crop:</label>
		<input type="text" name="bid2" id="bid2">
  
		<button type="submit">Submit</button>
	  </form>

  </body>
</html>
